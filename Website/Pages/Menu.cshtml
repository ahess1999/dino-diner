@page
@model Website.Pages.MenuModel
@{
    ViewData["Title"] = "Menu";
}

<h1>Menu</h1>
<h2>Combos</h2>
@for(int i = 0; i < Model.Menu.AvailableEntrees.Count; i++)
{
    <div class="menu-item">
        <p>@(Model.Menu.AvailableEntrees[i].ToString()) Combo</p>
    </div>
}
<h2>Entrees</h2>
<h2>Sides</h2>
<h2>Drinks</h2>

